		<deleteQueue name="string"/>

		<addDLQ deliveryQueue="string">
			<monitorAddress>string</monitorAddress>
		</addDLQ>
		<deleteDLQ deliveryQueue="string">
			<monitorAddress>string</monitorAddress>
		</deleteDLQ>

		<addDriver name="string">
			<description>string</description>
			<classpath>string</classpath>
			<module>string</module>
		</addDriver>
		<updateDriver name="string">
			<description>string</description>
			<classpath>string</classpath>
			<module>string</module>
		</updateDriver>
		<deleteDriver name="string"/>

		<addDatasource name="string" driverName="string" JTA="true">
			<jndi>string</jndi>
			<connectionUrl>string</connectionUrl>
			<username>string</username>
			<password>string</password>
		</addDatasource>
		<updateDatasource name="string" driverName="string" JTA="true">
			<jndi>string</jndi>
			<connectionUrl>string</connectionUrl>
			<username>string</username>
			<password>string</password>
		</updateDatasource>
