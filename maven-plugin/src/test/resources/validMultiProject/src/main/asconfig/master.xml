			<port>100</port>
			<user>string</user>
			<password>string</password>
			<strict>true</strict>
		</addMailSession>
		<updateMailSession name="string" jndi="string">
			<hostname>string</hostname>
			<port>100</port>
			<user>string</user>
			<password>string</password>
			<strict>true</strict>
		</updateMailSession>
		<deleteMailSession name="string"/>

		<addConnectionFactory name="string">
			<amountOfConnection>201</amountOfConnection>
			<jndi>string</jndi>
		</addConnectionFactory>
		<deleteConnectionFactory name="string"/>

		<changeLogLevel package="string" type="WARNING"/>
